        <EditTextPreference
            android:dialogTitle="@string/change_server_telephone"
            android:key="server_telephone"
            android:title="@string/server_telephone" />
    </PreferenceCategory>
    
    
    <PreferenceCategory android:title="@string/client" >
        <ListPreference
            android:defaultValue="21"
            android:dialogTitle="@string/change_font_size"
            android:entries="@array/font_size_entries"
            android:entryValues="@array/font_size_entry_values"
            android:key="font_size"
            android:title="@string/font_size" />

        <EditTextPreference
            android:dialogTitle="@string/change_client_telephone"
            android:key="client_telephone"
            android:title="@string/client_telephone" />

        <ListPreference
            android:defaultValue="gprs/umts preferred"
            android:dialogTitle="Change connection type"
            android:entries="@array/connection_type"
            android:entryValues="@array/connection_type_values"
            android:key="select_type"
            android:title="@string/sending_method" />

        <Preference
            android:key="insert_time"
            android:title="@string/time_before" />
        <Preference
            android:key="button_check_conn"
            android:title="@string/check_connection" />
        <Preference
            android:key="button_check_new_app"
            android:title="@string/check_new_app" />

        <Preference
            android:key="button_convert_submitted_to_complete"
            android:title="@string/convert_submitted_to_complete" />


        <ListPreference
            android:defaultValue="Disabled"
            android:dialogTitle="Edit Completed forms"
            android:entries="@array/editing_completed"
            android:entryValues="@array/editing_completed_values"
            android:key="button_completed_Editing"
            android:title="@string/complete_Editing" />
        <ListPreference
            android:defaultValue="Disabled"
            android:dialogTitle="Edit submitted forms"
            android:entries="@array/editing_completed"
            android:entryValues="@array/editing_completed_values"
            android:key="button_submitted_Editing"
            android:title="@string/submitted_Editing" />

        <ListPreference
        android:defaultValue="Disabled"
        android:dialogTitle="Keep Portrait screen only"
        android:entries="@array/editing_completed"
        android:entryValues="@array/editing_completed_values"
        android:key="button_Portrait"
        android:title="@string/portrait" />

        <ListPreference
            android:defaultValue="Disabled"
            android:dialogTitle="Enable Driving Mode"
            android:entries="@array/editing_completed"
            android:entryValues="@array/editing_completed_values"
            android:key="button_driving_mode"
            android:title="@string/dmode" />
