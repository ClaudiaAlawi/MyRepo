    <PreferenceCategory android:title="@string/server_preferences" >
        
        <ListPreference
            android:defaultValue="1"
            android:dialogTitle="@string/choose_protocol"
			android:entries="@array/protocol_choices_entries"
			android:entryValues="@array/protocol_choices_values"
			android:key="list_protocol"
			android:title="@string/portocol_choice"/>
        
        
        <EditTextPreference
            android:dialogTitle="@string/digit_server_ip"
            android:key="server_ip"
            android:title="@string/server_IP" />
        
        <EditTextPreference
            android:dialogTitle="@string/digit_server_port"
            android:key="server_port"
            android:title="@string/server_port" />

        <EditTextPreference
            android:dialogTitle="@string/digit_server_directory"
            android:key="server_directory"
            android:title="@string/server_directory" />
